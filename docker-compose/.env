# Middleware Name
MIDDLEWARE_HOST="adempiere-backend-rs.adempiere.middleware"
SERVER_PRIVATE_KEY="<Fill_It>"

# Networks
ADEMPIERE_NETWORK="adempiere-backend-rs_adempiere_network"
POSTGRES_NETWORK="adempiere-backend-rs_volume_postgres"