# Generic Restart
GENERIC_RESTART="always"
COMPOSE_PROJECT_NAME="adempiere-backend-rs"

# Database 
POSTGRES_IMAGE="postgres:13"
POSTGRES_NAME="adempiere-backend-rs.postgres.database"
